{
    "compilerOptions": {
        "outDir": "./output",
        "module": "CommonJS",
        "target": "es5",
        "noImplicitAny": true,
        "noImplicitThis":true,
        "alwaysStrict": true,
        // "noImplicitUseStrict": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "strictFunctionTypes": false,
        "baseUrl": ".",
        "paths": {
            "action":["action"],
            "config":["config"]
        }
    }
}