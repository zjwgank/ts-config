{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "es5",
        "outDir": "./output",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "strictFunctionTypes": true,
        "baseUrl": ".",
        "paths": {
            "config":["config"]
        },
        "typeRoots":["types"]
    }
}